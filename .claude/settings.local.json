{
  "permissions": {
      "allow": [
        "Bash(*)",
        "Edit(**/*)",
        "Write(**/*)",
        "Read(**/*)",
        "WebSearch",
        "WebFetch(*)"
      ],
      "deny": [],
      "ask": [
        "Bash(rm:*)",
        "Bash(rm -rf:*)",
        "Bash(rmdir:*)",
        "Bash(dd:*)",
        "Bash(mkfs:*)",
        "Bash(format:*)",
        "Bash(curl:*|bash)",
        "Bash(wget:*|bash)",
        "Bash(chmod 777:*)",
        "Bash(git push --force:*)",
        "Bash(git reset --hard:*)",
        "Bash(sudo:*)"
      ]
  }
}
