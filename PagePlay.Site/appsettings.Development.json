{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Debug"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Information",
        "Microsoft.EntityFrameworkCore": "Information",
        "System": "Warning"
      }
    }
  },
  "Security": {
    "PasswordPepper": "REPLACE_WITH_SECURE_VALUE",
    "Jwt": {
      "SecretKey": "REPLACE_WITH_SECURE_VALUE_AND_MUST_BE_AT_LEAST_32_CHARACTERS_LONG",
      "Issuer": "ArchTypeTodo",
      "Audience": "ArchTypeTodo",
      "ExpirationMinutes": 60
    }
  },
  "RateLimiting": {
    "RequestsPerMinute": 250,
    "ExcludedPaths": [
      "/health",
      "/_content",
      "/_framework"
    ]
  }
}
